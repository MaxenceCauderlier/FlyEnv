  "scripts": {
    "build-dev-runner": "cat scripts/args.esbuild.dev-runner | xargs npx esbuild",
    "web": "vite -c web/vite.config.ts",
    "web-build": "vite -c web/vite.config.ts build",
    "dev": "yarn run clean:dev && yarn run build-dev-runner && cross-env NODE_ENV=development TEST=electron node electron/dev-runner.js",
    "clean:dev": "rm -rf dist/*",
    "clean": "rm -rf dist/*;rm -rf node_modules/node-pty/build/*",
    "build": "yarn run clean && cat scripts/args.esbuild.app-builder | xargs npx esbuild && cross-env NODE_ENV=production node electron/app-builder.js",
    "build:arm64": "yarn run clean && cat scripts/args.esbuild.app-builder | xargs npx esbuild && cross-env NODE_ENV=production ARCH=arm64 node electron/app-builder.js",
    "build:amd64": "yarn run clean && cat scripts/args.esbuild.app-builder | xargs npx esbuild && cross-env NODE_ENV=production ARCH=amd64 node electron/app-builder.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "main": "./dist/electron/main.js",
  "optionalDependencies": {
    "fsevents": "*"
  },
